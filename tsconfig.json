{
  "compilerOptions": {
      "sourceMap": true,
      // TSはECMAScript 5に変換
      "target": "es5",
      // TSのモジュールはES Modulesとして出力
      "module": "es2015",
      // JSXの書式を有効に設定
      "jsx": "react",
      "allowJs": true,
      "moduleResolution": "node",
      "lib": [
      "es2018",
      "dom"
      ]
  }
}
