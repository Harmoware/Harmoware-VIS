{
  "name": "harmoware-vis",
  "description": "This is a minimal standalone version of the TripsLayer example on [deck.gl](http://deck.gl) website.",
  "version": "1.6.0",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "build:lib": "tsc --p tsconfig.json",
    "bus3d": "webpack-dev-server --progress --hot --port 3030 --open --content-base ./examples/bus3d --config ./examples/bus3d/webpack.config.js --mode development",
    "real-time-sample": "webpack-dev-server --progress --hot --port 8000 --open --content-base ./examples/real-time-sample --config ./examples/real-time-sample/webpack.config.js --mode development",
    "visualize-sample": "webpack-dev-server --progress --hot --port 3030 --open --content-base ./examples/visualize-sample --config ./examples/visualize-sample/webpack.config.js --mode development",
    "patch": "npm run build:lib && npm version patch && git push -u origin HEAD && node ./tools/tag.js",
    "minor": "npm run build:lib && npm version minor && git push -u origin HEAD && node ./tools/tag.js",
    "major": "npm run build:lib && npm version major && git push -u origin HEAD && node ./tools/tag.js"
  },
  "dependencies": {
    "@luma.gl/core": "^8.1.2",
    "deck.gl": "^8.1.4",
    "gl-matrix": "^2.8.1",
    "mapbox-gl": "1.5.1",
    "react-icons-kit": "^1.3.1",
    "react-map-gl": "^5.2.5",
    "react-spinners": "^0.8.1",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/axios": "^0.14.0",
    "@types/bootstrap": "^4.3.2",
    "@types/csvtojson": "^1.1.5",
    "@types/i18next": "^13.0.0",
    "@types/node-sass": "^4.11.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-i18next": "^8.1.0",
    "@types/react-map-gl": "^5.2.3",
    "@types/react-redux": "^7.1.7",
    "@types/redux": "^3.6.0",
    "@types/redux-saga": "^0.10.5",
    "@types/sass-loader": "^8.0.0",
    "@types/socket.io-client": "^1.4.32",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.4.1",
    "co": "^4.6.0",
    "css-loader": "^3.5.3",
    "csvtojson": "^2.0.10",
    "i18next": "^19.4.4",
    "node-sass": "^4.14.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "sass-loader": "^8.0.2",
    "socket.io-client": "^2.3.0",
    "style-loader": "^1.2.1",
