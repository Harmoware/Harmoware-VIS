{
  "name": "harmoware-vis",
  "description": "This is a minimal standalone version of the TripsLayer example on [deck.gl](http://deck.gl) website.",
  "version": "1.7.0",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "build:lib": "tsc --p tsconfig.json",
    "bus3d": "webpack-dev-server --progress --hot --port 3030 --open --content-base ./examples/bus3d --config ./examples/bus3d/webpack.config.js --mode development",
    "real-time-sample": "webpack-dev-server --progress --hot --port 8000 --open --content-base ./examples/real-time-sample --config ./examples/real-time-sample/webpack.config.js --mode development",
    "visualize-sample": "webpack-dev-server --progress --hot --port 3030 --open --content-base ./examples/visualize-sample --config ./examples/visualize-sample/webpack.config.js --mode development",
    "patch": "npm run build:lib && npm version patch && git push -u origin HEAD && node ./tools/tag.js",
    "minor": "npm run build:lib && npm version minor && git push -u origin HEAD && node ./tools/tag.js",
    "major": "npm run build:lib && npm version major && git push -u origin HEAD && node ./tools/tag.js"
  },
  "dependencies": {
    "@luma.gl/core": "^8.4.3",
    "deck.gl": "^8.4.7",
    "gl-matrix": "^3.3.0",
    "mapbox-gl": "^2.1.1",
    "react-icons-kit": "^1.3.1",
    "react-map-gl": "^6.1.9",
    "typescript-fsa": "^3.0.0",
    "typescript-fsa-reducers": "^1.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.0",
    "@babel/plugin-transform-runtime": "^7.13.6",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@loaders.gl/core": "^2.3.12",
    "@loaders.gl/gltf": "^2.3.12",
    "@loaders.gl/obj": "^2.3.12",
    "@types/bootstrap": "^5.0.5",
    "@types/csvtojson": "^1.1.5",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-map-gl": "^5.2.9",
    "@types/react-redux": "^7.1.16",
    "@types/sass-loader": "^8.0.0",
    "@types/socket.io-client": "^1.4.35",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "bootstrap": "^4.6.0",
    "co": "^4.6.0",
    "css-loader": "^5.0.2",
    "csvtojson": "^2.0.10",
    "i18next": "^19.8.9",
    "node-cmd": "^4.0.0",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.7",
    "react-redux": "^7.2.2",
    "react-spinners": "^0.10.6",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "sass-loader": "^10.1.1",
    "socket.io-client": "^3.1.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.2.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "author": "",
