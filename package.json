{
  "name": "harmoware-vis",
  "description": "This is a minimal standalone version of the TripsLayer example on [deck.gl](http://deck.gl) website.",
  "version": "1.1.1",
  "main": "lib/index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --hot --port 3030 --open --mode development",
    "build": "webpack --mode production",
    "lib": "babel src --out-dir lib",
    "flow": "flow",
    "bus3d": "webpack-dev-server --progress --hot --port 3030 --open --content-base ./examples/bus3d --config ./examples/bus3d/webpack.config.js --mode development",
    "visualize-sample": "webpack-dev-server --progress --hot --port 3030 --open --content-base ./examples/visualize-sample --config ./examples/visualize-sample/webpack.config.js --mode development",
    "visualize-sample-nonmap": "webpack-dev-server --progress --hot --port 3131 --open --content-base ./examples/visualize-sample-nonmap --config ./examples/visualize-sample-nonmap/webpack.config.js --mode development",
    "np": "np --no-cleanup",
    "deploy": "npm-run-all -s lib np"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "csvtojson": "^1.1.12",
    "deck.gl": "^4.1.2",
    "emotion": "^9.2.6",
    "gl-matrix": "^2.6.1",
    "i18next": "^11.3.6",
    "iconv-lite": "^0.4.23",
    "immutable": "^3.8.1",
    "luma.gl": "^4.0.2",
    "npm": "^6.1.0",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-i18next": "^7.8.1",
    "react-icons-kit": "^1.1.5",
    "react-map-gl": "^2.0.4",
    "react-modal": "^2.2.4",
    "react-redux": "^5.0.6",
    "react-spinners": "^0.3.3",
    "react-stats": "^0.0.5",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.76.0",
    "flowtype": "^2.0.0",
    "np": "^2.20.1",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
  },
  "author": "",
  "license": "ISC",
  "directories": {
    "example": "examples",
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Harmoware/Harmoware-VIS.git"
  },
  "bugs": {
    "url": "https://github.com/Harmoware/Harmoware-VIS/issues"
  },
  "homepage": "https://github.com/Harmoware/Harmoware-VIS#readme"
}
