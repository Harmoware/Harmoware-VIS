{
  "name": "harmoware-vis",
  "description": "This is a minimal standalone version of the TripsLayer example on [deck.gl](http://deck.gl) website.",
  "version": "1.0.0",
  "main": "lib/index.js",
  "scripts": {
    "start-local": "webpack-dev-server --env.local --progress --hot --port 3030 --open",
    "start": "webpack-dev-server --progress --hot --port 3030 --open",
    "build": "NODE_ENV=production webpack --env.prod=true",
    "lib": "babel src --out-dir lib",
    "flow": "flow",
    "bus3d": "webpack-dev-server --progress --hot --port 3030 --open --content-base ./examples/bus3d --config ./examples/bus3d/webpack.config.js",
    "visualize-sample": "webpack-dev-server --progress --hot --port 3030 --open --content-base ./examples/visualize-sample --config ./examples/visualize-sample/webpack.config.js"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "csvtojson": "^1.1.9",
    "deck.gl": "^4.1.2",
    "iconv-lite": "^0.4.19",
    "immutable": "^3.8.1",
    "luma.gl": "^4.0.2",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-map-gl": "^2.0.4",
    "react-modal": "^2.2.4",
    "react-redux": "^5.0.6",
    "react-stats": "^0.0.5",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-flowtype-errors": "^3.5.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.67.1",
    "flowtype": "^2.0.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3"
  },
  "author": "",
